{
  "version": 1,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
       "src": "/(.*)\\.(.*)",
       "dest": "/$1.$2"
     },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
